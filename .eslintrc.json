{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
      "standart", "prettier"
    ],
    "parser": "@typescript.eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint", "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
}
